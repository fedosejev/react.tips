<body class="landing-page">
  <%- include('../../__partials/subscribe-ad') %>

  <div class="container text-center">
    <%- include('../../__partials/header') %>

    <div class="row">
      <div class="col-sm-12">
        <h1>React Tutorials</h1>

        <ul class="posts">
          <% for (var iterator = 0; iterator < posts.length; iterator++) { if
          (posts[iterator].category === "react") { %>
          <li>
            <a href="/<%= posts[iterator].slug %>/">
              <%= posts[iterator].title %>
            </a>
          </li>
          <% } } %>
        </ul>
      </div>
    </div>

    <div class="row">
      <div class="col-sm-12">
        <h1>JavaScript Tutorials</h1>

        <ul class="posts">
          <% for (var iterator = 0; iterator < posts.length; iterator++) { if
          (posts[iterator].category === "javascript") { %>
          <li>
            <a href="/<%= posts[iterator].slug %>/">
              <%= posts[iterator].title %>
            </a>
          </li>
          <% } } %>
        </ul>
      </div>
    </div>

    <%- include('../../__partials/footer') %> <%-
    include('../../__partials/social') %>
  </div>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
  <script src="/__static/js/app.js"></script>

  <%- include('../../__partials/google-analytics') %>
</body>
